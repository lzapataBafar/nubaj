                actionCode, orderId, baseStore, customerId,
                c4cTicketId, ***************** DEFINIR EN TYPE ORDERS *****************
                actualDeliveryDate,
                paid, [NOTPAID, RETAINED, CANCELLED, PARTPAID, PAID]
                billed, [true, false]
                paymentDate, ***************** DEFINIR EN TYPE ORDERS *****************
